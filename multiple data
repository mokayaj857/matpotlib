import matplotlib.pyplot as plt 
names=[1,3,24,34,5,6,6]
year=range(1,8)
age=[20,21,19,16,13,67,39]
number=[1,2,4,5,6,7,8]
plt.plot(names,year,color='blue',marker='o',linestyle='dashed',linewidth=3,label='my_life')
plt.plot(age,year,label='my_age',linestyle='dashdot')
plt.plot(number,year,label='my_number')
plt.legend()
plt.title('my biography')
plt.show().
